<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
   "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<constant name="struts.devMode" value="true"/>
	<constant name="struts.enable.SlashesInActionNames" value="true"/>
	<constant name="struts.mapper.alwaysSelectFullNamespace" value="false"/>

	<package name="default" extends="struts-default">

		<action name="pret-topo" class="fr.silvharm.commulade.webapp.action.PretTopoAction" method="execute">
			<result name="success">/jsp/template/main-template.jsp</result>
		</action>

		<action name="recherche-site" class="rechercheSiteAction" method="execute">
			<result name="success">/jsp/template/main-template.jsp</result>
		</action>

		<action name="topo-presentation" class="fr.silvharm.commulade.webapp.action.TopoPresentationAction" method="execute">
			<result name="success">/jsp/template/main-template.jsp</result>
		</action>

		<action name="welcome" class="fr.silvharm.commulade.webapp.action.WelcomeAction" method="execute">
			<result name="success">/jsp/template/main-template.jsp</result>
		</action>

	</package>
</struts>
