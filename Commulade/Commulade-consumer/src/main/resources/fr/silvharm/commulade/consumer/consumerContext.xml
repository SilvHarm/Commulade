<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="abstractDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.AbstractDaoImpl"
		abstract="true">
		<property name="jdbcTemplate" ref="dataSourceCommulade"/>
	</bean>


	<bean id="commentDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.CommentDaoImpl"
		parent="abstractDaoImpl"></bean>

	<bean id="lendingTopoDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.LendingTopoDaoImpl"
		parent="abstractDaoImpl"></bean>

	<bean id="longueurDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.LongueurDaoImpl"
		parent="abstractDaoImpl"></bean>

	<bean id="messageDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.MessageDaoImpl"
		parent="abstractDaoImpl"></bean>

	<bean id="secteurDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.SecteurDaoImpl"
		parent="abstractDaoImpl"></bean>

	<bean id="siteDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.SiteDaoImpl" parent="abstractDaoImpl"></bean>

	<bean id="topoDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.TopoDaoImpl" parent="abstractDaoImpl"></bean>

	<bean id="userDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.UserDaoImpl" parent="abstractDaoImpl"></bean>

	<bean id="voieDaoImpl" class="fr.silvharm.commulade.consumer.impl.dao.VoieDaoImpl" parent="abstractDaoImpl"></bean>

</beans>
